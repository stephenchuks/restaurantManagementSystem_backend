{
    "name": "my-menu-platform-backend",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "engines": {
      "node": ">=18"
    },
    "scripts": {
      "build": "tsc -b",
      "start": "node ./dist/server.js",
      "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node-esm' src/server.ts",
      "lint": "eslint 'src/**/*.ts' --fix",
      "format": "prettier --write .",
      "test": "jest --passWithNoTests",
      "test:watch": "jest --watch"
    },
    "dependencies": {
      "express": "^4.18.2",
      "dotenv": "^16.1.0",
      "class-validator": "^0.14.0",
      "class-transformer": "^0.6.1",
      "mongoose": "^7.3.0",
      "ioredis": "^5.3.2",
      "jsonwebtoken": "^9.0.0",
      "qrcode": "^1.5.1"
    },
    "devDependencies": {
      "typescript": "^5.1.6",
      "ts-node-esm": "^10.9.1",
      "@types/node": "^20.1.0",
      "@types/express": "^4.17.17",
      "@typescript-eslint/parser": "^6.3.0",
      "@typescript-eslint/eslint-plugin": "^6.3.0",
      "eslint": "^8.34.0",
      "eslint-config-prettier": "^9.0.0",
      "eslint-plugin-import": "^2.29.4",
      "prettier": "^3.0.0",
      "jest": "^29.5.0",
      "ts-jest": "^29.1.0",
      "@types/jest": "^29.4.0",
      "nodemon": "^2.0.22"
    }
  }
  